{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "main": "index.js",
  "scripts": {
    "local": "SERVICE_ENV=local node index.js",
    "start": "node index.js",
    "test-ci": "SERVICE_ENV=test mocha --reporter mocha-junit-reporter",
    "test:report": "SERVICE_ENV=test nyc --reporter=lcovonly mocha --reporter mocha-junit-reporter './test/**/*.test.js'",
    "test": "SERVICE_ENV=test mocha --reporter spec './test/**/*.test.js'",
    "lint": "eslint .",
    "manifest": "node_modules/make-manifest/bin/make-manifest",
    "qa": "npm run lint && npm run test",
    "postinstall": "node_modules/.bin/eslint . --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mocha": "^5.2.0",
    "expect.js": "^0.3.1",
    "husky": "^4.2.3",
    "make-manifest": "^1.0.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "on-headers": "^1.0.1",
    "body-parser": "^1.15.2",
    "boom": "^3.2.2",
    "bunyan": "^1.8.5",
    "chalk": "^1.1.3",
    "confabulous": "^1.1.0",
    "debug": "^2.2.0",
    "express-swagger-generator": "^1.1.15",
    "hogan.js": "^3.0.2",
    "make-manifest": "^1.0.1",
    "optimist": "^0.6.1",
    "optional": "^0.1.3",
    "prepper": "^1.1.0",
    "ramda": "^0.26.1",
    "systemic": "^3.0.0",
    "systemic-express": "^1.1.0",
    "systemic-domain-runner": "^1.1.0",
    "systemic-azure-metrics": "^0.1.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  }
}
