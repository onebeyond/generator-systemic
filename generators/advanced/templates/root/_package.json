{
  "name": "<%= name %>",
  "version": "0.0.1",
  "description": "<%= description %>",
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>"
  },
  "main": "index.js",
  "scripts": {
    "local": "SERVICE_ENV=local node index.js",
    "start": "node index.js",
    "test-ci": "SERVICE_ENV=test mocha --reporter mocha-junit-reporter",
    "test": "SERVICE_ENV=test mocha --reporter spec './test/**/*.test.js'",
    "lint": "eslint .",
    "manifest": "node_modules/make-manifest/bin/make-manifest",
    "qa": "npm run lint && npm run test",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "postinstall": "node_modules/.bin/eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^6.2.2",
    "expect.js": "^0.3.1",
    "husky": "^3.1.0",
    "make-manifest": "^1.0.1",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "on-headers": "^1.0.2",
    "body-parser": "^1.19.0",
    "@hapi/boom": "^9.1.0",
    "bunyan": "^1.8.12",
    "chalk": "^4.0.0",
    "confabulous": "^1.7.0",
    "debug": "^4.1.1",
    "hogan.js": "^3.0.2",
    "make-manifest": "^1.0.1",
    "optimist": "^0.6.1",
    "optional": "^0.1.4",
    "prepper": "^1.2.0",
    "ramda": "^0.27.0",
    "swagger-endpoint-validator": "^2.0.2",
    "systemic": "^3.3.7",
    "systemic-express": "^1.1.1",
    "systemic-domain-runner": "^1.1.0",
    "systemic-mongodb": "^1.0.5"
  }
}
